<widgets>


<enrichmentwidgetdisplayer id="disease_enrichment_for_feature"
    title="Disease Enrichment"
    description="Disease terms enriched for items in this list."
    label="Disease Term"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm[MedicTerm].parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	ontologyAnnotations.qualifier = null"
    views="symbol,name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, 
	ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,
	ontologyAnnotations.ontologyTerm.parents.name"
    />

<enrichmentwidgetdisplayer id="mp_enrichment_for_feature"
    title="Mammalian Phenotype Ontology Enrichment"
    description="MP terms enriched for items in this list."
    label="MP Term"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm[MPTerm].parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	ontologyAnnotations.qualifier = null"
    views="symbol,name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, 
	ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,
	ontologyAnnotations.ontologyTerm.parents.name"
    externalLink="http://www.informatics.jax.org/searches/Phat.cgi?id="/>

 <enrichmentwidgetdisplayer id="go_enrichment_for_feature"
    title="Gene Ontology Enrichment"
    description="GO terms enriched for items in this list."
    label="GO Term"
    filterLabel="Ontology"
    filters="biological_process,cellular_component,molecular_function"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm.parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="ontologyAnnotations.ontologyTerm.parents.namespace=[Ontology],organism.name=[list],
	primaryIdentifier != null, ontologyAnnotations.qualifier = null,
	ontologyAnnotations.dataSets.name = GOTerm to Mouse Marker Annotations from MGI,
	ontologyAnnotations.evidence.code.code != ND,
	ontologyAnnotations.ontologyTerm.parents.identifier != go:0008150,
	ontologyAnnotations.ontologyTerm.parents.identifier != go:0003674,
	ontologyAnnotations.ontologyTerm.parents.identifier != go:0005575"
    views="symbol,name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,ontologyAnnotations.ontologyTerm.parents.name"
    externalLink="http://www.informatics.jax.org/searches/GO.cgi?id="/>

<enrichmentwidgetdisplayer id="emapa_enrichment_for_feature"
    title="Anatomy Enrichment"
    description="Anatomy (EMAPA) terms enriched for items in this list."
    label="EMAPA Term"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    startClassDisplay="primaryIdentifier"
    enrich="expression.structure.parents.name"
    enrichIdentifier="expression.structure.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	expression.strength != Absent,
	expression.strength != Ambiguous"
    views="symbol,name,primaryIdentifier,organism.name,
	expression.structure.identifier,
	expression.structure.name,
	expression.strength,
	expression.pattern,
	expression.structure.parents.identifier,
	expression.structure.parents.name" />

<graphdisplayer id="chromosome_distribution_for_feature"
    title="Chromosome Distribution"
    description="Actual: number of items in this list found on each chromosome. Expected: given the total number of items on the chromosome and the number of items in this list, the number of items expected to be found on each chromosome."
    graphType="ColumnChart"
    rangeLabel="Count"
    domainLabel="Chromosome"
    filterLabel="Organism"
    filters="organism.name=[list]"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    categoryPath="chromosome.symbol"
    seriesLabels="Actual,Expected"
    seriesPath="ActualExpectedCriteria"
    constraints="organism.name=[Organism]"
    views="primaryIdentifier, symbol, organism.name, chromosome.primaryIdentifier,
	chromosomeLocation.start, chromosomeLocation.end, chromosomeLocation.strand"/>

<enrichmentwidgetdisplayer id="mp_enrichment_for_genotype"
    title="Mammalian Phenotype Ontology Enrichment"
    description="MP terms enriched for items in this list."
    label="MP Term"
    startClass="Genotype"
    typeClass="Genotype"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm.parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	ontologyAnnotations.dataSets.name = MPTerm to Mouse Genotype Annotations from MGI,
	ontologyAnnotations.qualifier = null"
    views="symbol,background.name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, 
	ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,
	ontologyAnnotations.ontologyTerm.parents.name"
    externalLink="http://www.informatics.jax.org/searches/Phat.cgi?id="/>

 <enrichmentwidgetdisplayer id="disease_enrichment_for_genotype"
    title="Human Disease Enrichment"
    description="Disease terms enriched for items in this list."
    label="Disease Term"
    startClass="Genotype"
    typeClass="Genotype"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm[DiseaseTerm].parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	ontologyAnnotations.qualifier = null"
    views="symbol,background.name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, 
	ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,
	ontologyAnnotations.ontologyTerm.parents.name" />

</widgets>  
</webconfig>
