<template-queries>
<template name="Coords_Features" title="Coordinates --&gt; Features" comment="">
  <query name="Coords_Features" model="genomic" view="SequenceFeature.chromosomeLocation.locatedOn.primaryIdentifier SequenceFeature.chromosomeLocation.start SequenceFeature.chromosomeLocation.end SequenceFeature.chromosomeLocation.strand SequenceFeature.organism.shortName SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.sequenceOntologyTerm.name" longDescription="Returns genome features contained within a specified coordinate range for the specified species. &lt;br&gt;&lt;strong&gt;(See also the &lt;a href=&quot;genomicRegionSearch.do&quot;&gt;Region Search&lt;/a&gt; tab.)&lt;/strong&gt;" sortOrder="SequenceFeature.chromosomeLocation.start asc" constraintLogic="A and C and D and E">
    <constraint path="SequenceFeature.chromosomeLocation.locatedOn" editable="false" type="Chromosome"/>
    <constraint path="SequenceFeature.organism.shortName" code="E" editable="true" op="=" value="M. musculus"/>
    <constraint path="SequenceFeature.chromosomeLocation.locatedOn.primaryIdentifier" code="A" editable="true" description="" op="=" value="12"/>
    <constraint path="SequenceFeature.chromosomeLocation.start" code="D" editable="true" description="" op="&gt;=" value="10000000"/>
    <constraint path="SequenceFeature.chromosomeLocation.end" code="C" editable="true" description="" op="&lt;=" value="12000000"/>
  </query>
</template>
<template name="Feature_crossrefs" title="Features --&gt; Cross References" comment="">
  <query name="Feature_crossrefs" model="genomic" view="SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.crossReferences.identifier SequenceFeature.crossReferences.source.name" longDescription="Returns database xrefs/links for the specified genome features." sortOrder="SequenceFeature.primaryIdentifier asc">
    <constraint path="SequenceFeature" editable="true" op="LOOKUP" value="Pax6" extraValue="M. musculus"/>
  </query>
</template>
<template name="Feature_overlapping" title="Features --&gt; Overlapping features" comment="">
  <query name="Feature_overlapping" model="genomic" view="SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.chromosomeLocation.locatedOn.primaryIdentifier SequenceFeature.chromosomeLocation.start SequenceFeature.chromosomeLocation.end SequenceFeature.chromosomeLocation.strand SequenceFeature.overlappingFeatures.primaryIdentifier SequenceFeature.overlappingFeatures.symbol SequenceFeature.overlappingFeatures.sequenceOntologyTerm.name SequenceFeature.overlappingFeatures.chromosomeLocation.locatedOn.primaryIdentifier SequenceFeature.overlappingFeatures.chromosomeLocation.start SequenceFeature.overlappingFeatures.chromosomeLocation.end SequenceFeature.overlappingFeatures.chromosomeLocation.strand" longDescription="Returns genome features that overlap the specified features." sortOrder="SequenceFeature.symbol asc SequenceFeature.overlappingFeatures.symbol asc">
    <constraint path="SequenceFeature" editable="true" op="LOOKUP" value="Bmp2, Bmp4" extraValue="M. musculus"/>
  </query>
</template>
<template name="Gene_Expression" title="Gene --&gt; Expression annotations" comment="">
  <query name="Gene_Expression" model="genomic" view="GXDExpression.assayType GXDExpression.feature.symbol GXDExpression.feature.primaryIdentifier GXDExpression.structure.namespace GXDExpression.structure.name GXDExpression.strength GXDExpression.pattern GXDExpression.genotype.symbol GXDExpression.sex GXDExpression.age GXDExpression.assayId GXDExpression.probe GXDExpression.image GXDExpression.publication.mgiJnum" longDescription="Returns expression annotations for the specified gene(s)." sortOrder="GXDExpression.assayId asc" constraintLogic="A and B">
    <constraint path="GXDExpression.feature.organism.taxonId" code="B" editable="false" description="" op="=" value="10090"/>
    <constraint path="GXDExpression.feature" code="A" editable="true" op="LOOKUP" value="Pax6" extraValue=""/>
  </query>
</template>
<template name="Gene_Ortholog" title="Genes --&gt; Homologs" comment="">
  <query name="Gene_Ortholog" model="genomic" view="Gene.primaryIdentifier Gene.symbol Gene.organism.name Gene.homologues.homologue.primaryIdentifier Gene.homologues.homologue.symbol Gene.homologues.homologue.organism.name Gene.homologues.type Gene.homologues.dataSets.name" longDescription="Returns homologs of specified genes." sortOrder="Gene.primaryIdentifier asc">
    <constraint path="Gene" editable="true" op="LOOKUP" value="C4a, Sox9" extraValue="M. musculus"/>
  </query>
</template>
<template name="GO_MFeatures" title="Functions (GO terms and children) --&gt; Mouse features" comment="">
  <query name="GO_MFeatures" model="genomic" view="SequenceFeature.ontologyAnnotations.ontologyTerm.namespace SequenceFeature.ontologyAnnotations.ontologyTerm.identifier SequenceFeature.ontologyAnnotations.ontologyTerm.name SequenceFeature.ontologyAnnotations.qualifier SequenceFeature.ontologyAnnotations.evidence.code.code SequenceFeature.ontologyAnnotations.evidence.withText SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.mgiType SequenceFeature.ontologyAnnotations.evidence.publications.mgiJnum SequenceFeature.ontologyAnnotations.evidence.publications.pubMedId SequenceFeature.ontologyAnnotations.evidence.publications.citation" longDescription="Returns mouse features associated with specified GO terms (and any children of this GO term)." sortOrder="SequenceFeature.mgiType asc" constraintLogic="A and D and C">
    <constraint path="SequenceFeature.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="GOTerm to Mouse Marker Annotations from MGI"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.organism.taxonId" code="D" editable="false" description="" op="=" value="10090"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" code="C" editable="true" op="LOOKUP" value="DNA repair" extraValue=""/>
  </query>
</template>
<template name="GO_MFeatures_NoChildren" title="Functions (GO terms) --&gt; Mouse features" comment="">
  <query name="GO_MFeatures_NoChildren" model="genomic" view="SequenceFeature.ontologyAnnotations.ontologyTerm.namespace SequenceFeature.ontologyAnnotations.ontologyTerm.identifier SequenceFeature.ontologyAnnotations.ontologyTerm.name SequenceFeature.ontologyAnnotations.qualifier SequenceFeature.ontologyAnnotations.evidence.code.code SequenceFeature.ontologyAnnotations.evidence.withText SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.mgiType SequenceFeature.ontologyAnnotations.evidence.publications.mgiJnum SequenceFeature.ontologyAnnotations.evidence.publications.pubMedId SequenceFeature.ontologyAnnotations.evidence.publications.citation" longDescription="Returns mouse features associated with specified GO terms." sortOrder="SequenceFeature.ontologyAnnotations.ontologyTerm.name asc" constraintLogic="A and D and C">
    <constraint path="SequenceFeature.organism.taxonId" code="D" editable="false" description="" op="=" value="10090"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="GOTerm to Mouse Marker Annotations from MGI"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" code="C" editable="true" op="LOOKUP" value="DNA repair" extraValue=""/>
  </query>
</template>
<template name="GO_MFeatures_NoNamespace" title="Functions (GO terms and children) --&gt; Mouse features" comment="">
  <query name="GO_MFeatures_NoNamespace" model="genomic" view="SequenceFeature.ontologyAnnotations.ontologyTerm.identifier SequenceFeature.ontologyAnnotations.ontologyTerm.name SequenceFeature.ontologyAnnotations.qualifier SequenceFeature.ontologyAnnotations.evidence.code.code SequenceFeature.ontologyAnnotations.evidence.withText SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.mgiType SequenceFeature.ontologyAnnotations.evidence.publications.mgiJnum SequenceFeature.ontologyAnnotations.evidence.publications.pubMedId SequenceFeature.ontologyAnnotations.evidence.publications.citation" longDescription="Returns mouse features associated with specified GO terms (and any children of this GO term)." sortOrder="SequenceFeature.mgiType asc" constraintLogic="A and D and C">
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.organism.taxonId" code="D" editable="false" description="" op="=" value="10090"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="GOTerm to Mouse Marker Annotations from MGI"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" code="C" editable="true" op="LOOKUP" value="DNA repair" extraValue=""/>
  </query>
</template>
<template name="GO_MFeatures_NoPubs" title="Functions (GO terms and children) --&gt; Mouse features" comment="">
  <query name="GO_MFeatures_NoPubs" model="genomic" view="SequenceFeature.ontologyAnnotations.ontologyTerm.namespace SequenceFeature.ontologyAnnotations.ontologyTerm.identifier SequenceFeature.ontologyAnnotations.ontologyTerm.name SequenceFeature.ontologyAnnotations.qualifier SequenceFeature.ontologyAnnotations.evidence.code.code SequenceFeature.ontologyAnnotations.evidence.withText SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.mgiType" longDescription="Returns mouse features associated with specified GO terms (and any children of this GO term)." sortOrder="SequenceFeature.mgiType asc" constraintLogic="A and D and C">
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.organism.taxonId" code="D" editable="false" description="" op="=" value="10090"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="GOTerm to Mouse Marker Annotations from MGI"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm.parents" code="C" editable="true" op="LOOKUP" value="DNA repair" extraValue=""/>
  </query>
</template>
<template name="GO_Publications" title="Functions (GO terms and children) --&gt; Publications" comment="">
  <query name="GO_Publications" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.ontologyAnnotations.evidence.code.code GOTerm.ontologyAnnotations.evidence.withText GOTerm.ontologyAnnotations.evidence.publications.mgiJnum GOTerm.ontologyAnnotations.evidence.publications.pubMedId GOTerm.ontologyAnnotations.evidence.publications.firstAuthor GOTerm.ontologyAnnotations.evidence.publications.journal GOTerm.ontologyAnnotations.evidence.publications.title GOTerm.ontologyAnnotations.evidence.publications.year GOTerm.ontologyAnnotations.qualifier" longDescription="Returns publications related to specified function (GO) terms, i.e., those cited by annotations involving those terms  (and any children of the GO terms)." sortOrder="GOTerm.ontologyAnnotations.evidence.publications.year desc">
    <constraint path="GOTerm.parents" editable="false" type="GOTerm"/>
    <constraint path="GOTerm.parents" editable="true" op="LOOKUP" value="homeostatic process" extraValue=""/>
  </query>
</template>
<template name="GO_Publications_NoChildren" title="Functions (GO terms) --&gt; Publications" comment="">
  <query name="GO_Publications_NoChildren" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.ontologyAnnotations.evidence.code.code GOTerm.ontologyAnnotations.evidence.withText GOTerm.ontologyAnnotations.evidence.publications.mgiJnum GOTerm.ontologyAnnotations.evidence.publications.pubMedId GOTerm.ontologyAnnotations.evidence.publications.firstAuthor GOTerm.ontologyAnnotations.evidence.publications.journal GOTerm.ontologyAnnotations.evidence.publications.title GOTerm.ontologyAnnotations.evidence.publications.year" longDescription="Returns publications related to specified function (GO) terms, i.e., those cited by annotations involving those terms." sortOrder="GOTerm.identifier asc">
    <constraint path="GOTerm" editable="true" op="LOOKUP" value="homeostatic process" extraValue=""/>
  </query>
</template>
<template name="HDisease_HGene" title="Diseases (MEDIC terms) --&gt; Human genes" comment="">
  <query name="HDisease_HGene" model="genomic" view="Gene.ontologyAnnotations.ontologyTerm.identifier Gene.ontologyAnnotations.ontologyTerm.name Gene.primaryIdentifier Gene.symbol" longDescription="Returns human genes associated with specified human diseases." sortOrder="Gene.ontologyAnnotations.ontologyTerm.name asc" constraintLogic="B and A">
    <constraint path="Gene.ontologyAnnotations.dataSets.name" code="B" editable="false" description="" op="=" value="DiseaseTerm to Human Marker Annotations from MGI"/>
    <constraint path="Gene.ontologyAnnotations.ontologyTerm" editable="false" type="MedicTerm"/>
    <constraint path="Gene.ontologyAnnotations.ontologyTerm.parents" editable="false" type="MedicTerm"/>
    <constraint path="Gene.ontologyAnnotations.ontologyTerm.parents" code="A" editable="true" op="LOOKUP" value="*diabetes*" extraValue=""/>
  </query>
</template>
<template name="HDisease_MModel" title="Diseases (MEDIC terms) --&gt; Mouse models" comment="">
  <query name="HDisease_MModel" model="genomic" view="Gene.alleles.genotypes.ontologyAnnotations.ontologyTerm.identifier Gene.alleles.genotypes.ontologyAnnotations.ontologyTerm.name Gene.primaryIdentifier Gene.symbol Gene.alleles.genotypes.symbol Gene.alleles.genotypes.background.name" longDescription="Returns mouse genes associated with (modeling) specified human diseases." sortOrder="Gene.symbol asc" constraintLogic="B and A and C and D and E and F and G">
    <constraint path="Gene.alleles.genotypes.ontologyAnnotations.qualifier" code="G" editable="false" description="" op="IS NULL"/>
    <constraint path="Gene.primaryIdentifier" code="F" editable="false" description="" op="!=" value="MGI:104735"/>
    <constraint path="Gene.alleles.isRecombinase" code="E" editable="false" description="" op="=" value="false"/>
    <constraint path="Gene.alleles.genotypes.zygosity" code="D" editable="false" description="" op="!=" value="cx"/>
    <constraint path="Gene.alleles.isWildType" code="C" editable="false" description="" op="=" value="false"/>
    <constraint path="Gene.alleles.genotypes.ontologyAnnotations.dataSets.name" code="B" editable="false" description="" op="=" value="DiseaseTerm to Mouse Genotype Annotations from MGI"/>
    <constraint path="Gene.alleles.genotypes.ontologyAnnotations.ontologyTerm.parents" editable="false" type="MedicTerm"/>
    <constraint path="Gene.alleles.genotypes.ontologyAnnotations.ontologyTerm" editable="false" type="MedicTerm"/>
    <constraint path="Gene.alleles.genotypes.ontologyAnnotations.ontologyTerm.parents" code="A" editable="true" op="LOOKUP" value="parkinson*" extraValue=""/>
  </query>
</template>
<template name="HDisease_Publications" title="Diseases (MEDIC terms) --&gt; Publications" comment="">
  <query name="HDisease_Publications" model="genomic" view="MedicTerm.identifier MedicTerm.name MedicTerm.ontologyAnnotations.evidence.publications.mgiJnum MedicTerm.ontologyAnnotations.evidence.publications.pubMedId MedicTerm.ontologyAnnotations.evidence.publications.firstAuthor MedicTerm.ontologyAnnotations.evidence.publications.journal MedicTerm.ontologyAnnotations.evidence.publications.title MedicTerm.ontologyAnnotations.evidence.publications.year" longDescription="Returns mouse model publications for the specified disease (MEDIC) terms." sortOrder="MedicTerm.name asc" constraintLogic="A and B">
    <constraint path="MedicTerm.parents" editable="false" type="MedicTerm"/>
    <constraint path="MedicTerm.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="DiseaseTerm to Mouse Genotype Annotations from MGI"/>
    <constraint path="MedicTerm.parents" code="B" editable="true" op="LOOKUP" value="Parkinson*" extraValue=""/>
  </query>
</template>
<template name="HGene_HDisease" title="Human genes --&gt; Diseases (MEDIC) terms" comment="">
  <query name="HGene_HDisease" model="genomic" view="Gene.primaryIdentifier Gene.symbol Gene.ontologyAnnotations.ontologyTerm.identifier Gene.ontologyAnnotations.ontologyTerm.name" longDescription="Returns human diseases (MEDIC terms) associated with specified human genes." sortOrder="Gene.ontologyAnnotations.ontologyTerm.name asc" constraintLogic="A and B and C">
    <constraint path="Gene.organism.taxonId" code="C" editable="false" description="" op="=" value="9606"/>
    <constraint path="Gene.ontologyAnnotations.dataSets.name" code="B" editable="false" description="" op="=" value="DiseaseTerm to Human Marker Annotations from MGI"/>
    <constraint path="Gene.ontologyAnnotations.ontologyTerm" editable="false" type="MedicTerm"/>
    <constraint path="Gene" code="A" editable="true" op="LOOKUP" value="CFTR" extraValue="H. sapiens"/>
  </query>
</template>
<template name="HGene_MPhenotype" title="Human genes --&gt; Phenotypes of mouse orthologs" comment="">
  <query name="HGene_MPhenotype" model="genomic" view="Gene.primaryIdentifier Gene.symbol Gene.organism.name Gene.homologues.homologue.primaryIdentifier Gene.homologues.homologue.symbol Gene.homologues.homologue.alleles.genotypes.ontologyAnnotations.ontologyTerm.identifier Gene.homologues.homologue.alleles.genotypes.ontologyAnnotations.ontologyTerm.name Gene.homologues.homologue.organism.name" longDescription="Returns the phenotypes of mice with mutations in the orthologs of the specified human genes." sortOrder="Gene.homologues.homologue.primaryIdentifier asc" constraintLogic="A and C and D and F and G and H and I and J">
    <constraint path="Gene.homologues.homologue.alleles.genotypes.ontologyAnnotations.qualifier" code="I" editable="false" description="" op="IS NULL"/>
    <constraint path="Gene.homologues.homologue.alleles.genotypes.zygosity" code="H" editable="false" description="" op="!=" value="cx"/>
    <constraint path="Gene.homologues.homologue.alleles.isRecombinase" code="G" editable="false" description="" op="=" value="false"/>
    <constraint path="Gene.homologues.homologue.alleles.isWildType" code="F" editable="false" description="" op="=" value="false"/>
    <constraint path="Gene.homologues.homologue.alleles.genotypes.ontologyAnnotations.ontologyTerm" editable="false" type="MPTerm"/>
    <constraint path="Gene.organism.taxonId" code="D" editable="false" description="" op="=" value="9606"/>
    <constraint path="Gene.homologues.homologue.alleles.genotypes.ontologyAnnotations.dataSets.name" code="C" editable="false" description="" op="=" value="MPTerm to Mouse Genotype Annotations from MGI"/>
    <constraint path="Gene.homologues.homologue.organism.taxonId" code="J" editable="false" description="" op="=" value="10090"/>
    <constraint path="Gene" code="A" editable="true" op="LOOKUP" value="BMP4" extraValue="H. sapiens"/>
  </query>
</template>
<template name="Lookup_Features" title="Lookup --&gt; Features" comment="">
  <query name="Lookup_Features" model="genomic" view="SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.sequenceOntologyTerm.name SequenceFeature.chromosomeLocation.locatedOn.primaryIdentifier SequenceFeature.chromosomeLocation.start SequenceFeature.chromosomeLocation.end SequenceFeature.chromosomeLocation.strand SequenceFeature.organism.shortName" longDescription="Returns genome features that match a specified string. (ID, symbol, name, synonym) for the specified species." sortOrder="SequenceFeature.symbol asc" constraintLogic="A and B">
    <constraint path="SequenceFeature" code="A" editable="true" op="LOOKUP" value="Adh*" extraValue="M. musculus"/>
    <constraint path="SequenceFeature.organism.shortName" code="B" editable="true" op="=" value="M. musculus"/>
  </query>
</template>
<template name="Lookup_GO" title="Lookup --&gt; Functions (GO terms)" comment="">
  <query name="Lookup_GO" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.description GOTerm.namespace" longDescription="Returns GO terms that match specified search string." sortOrder="GOTerm.identifier asc" constraintLogic="A and B">
    <constraint path="GOTerm.obsolete" code="B" editable="false" description="" op="=" value="false"/>
    <constraint path="GOTerm.name" code="A" editable="true" op="CONTAINS" value="homeostasis"/>
  </query>
</template>
<template name="Lookup_HDisease" title="Lookup --&gt; Disease (MEDIC) terms" comment="">
  <query name="Lookup_HDisease" model="genomic" view="MedicTerm.identifier MedicTerm.name MedicTerm.description" longDescription="Returns human disease (MEDIC) terms the match the specified search string." sortOrder="MedicTerm.name asc" constraintLogic="A and B">
    <constraint path="MedicTerm.obsolete" code="A" editable="false" description="" op="=" value="false"/>
    <constraint path="MedicTerm.name" code="B" editable="true" op="CONTAINS" value="autoimmune"/>
  </query>
</template>
<template name="Lookup_MAlleles" title="Lookup --&gt; Mouse Alleles" comment="">
  <query name="Lookup_MAlleles" model="genomic" view="Allele.primaryIdentifier Allele.symbol Allele.name Allele.alleleType" longDescription="Returns mouse alleles matching the given string." sortOrder="Allele.symbol asc" constraintLogic="A and B">
    <constraint path="Allele.organism.taxonId" code="B" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele" code="A" editable="true" op="LOOKUP" value="Pax6&lt;Sey-Dey&gt;" extraValue="M. musculus"/>
  </query>
</template>
<template name="Lookup_MPhenotype" title="Lookup --&gt; Mammalian phenotypes (MP terms)" comment="">
  <query name="Lookup_MPhenotype" model="genomic" view="MPTerm.name MPTerm.identifier MPTerm.description" longDescription="Returns MP terms whose names match the specified search terms." sortOrder="MPTerm.name asc" constraintLogic="A and B">
    <constraint path="MPTerm.obsolete" code="B" editable="false" description="" op="=" value="false"/>
    <constraint path="MPTerm.name" code="A" editable="true" op="CONTAINS" value="hemoglobin"/>
  </query>
</template>
<template name="MAllele_Genotype" title="Mouse alleles --&gt; Genotypes (models)" comment="">
  <query name="MAllele_Genotype" model="genomic" view="Allele.primaryIdentifier Allele.symbol Allele.name Allele.genotypes.primaryIdentifier Allele.genotypes.zygosity Allele.genotypes.symbol Allele.genotypes.background.name Allele.genotypes.background.strainType" longDescription="Returns genotypes (mouse models) carrying any of the specified alleles." sortOrder="Allele.symbol asc" constraintLogic="A and B">
    <constraint path="Allele.organism.taxonId" code="B" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele" code="A" editable="true" op="LOOKUP" value="Pax6&lt;Sey-Dey&gt;" extraValue="M. musculus"/>
  </query>
</template>
<template name="MAllele_HDisease" title="Mouse alleles --&gt; Human diseases (MEDIC terms)" comment="">
  <query name="MAllele_HDisease" model="genomic" view="Allele.primaryIdentifier Allele.symbol Allele.name Allele.genotypes.ontologyAnnotations.ontologyTerm.identifier Allele.genotypes.ontologyAnnotations.ontologyTerm.name" longDescription="Returns human diseases (MEDIC terms) modeled by mice carrying specified alleles." sortOrder="Allele.genotypes.ontologyAnnotations.ontologyTerm.name asc" constraintLogic="A and B and D and C and E and F and G and H">
    <constraint path="Allele.isRecombinase" code="H" editable="false" description="" op="=" value="false"/>
    <constraint path="Allele.genotypes.ontologyAnnotations.qualifier" code="G" editable="false" description="" op="IS NULL"/>
    <constraint path="Allele.genotypes.zygosity" code="F" editable="false" description="" op="!=" value="cx"/>
    <constraint path="Allele.isWildType" code="E" editable="false" description="" op="=" value="false"/>
    <constraint path="Allele.genotypes.organism.taxonId" code="B" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele.genotypes.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="DiseaseTerm to Mouse Genotype Annotations from MGI"/>
    <constraint path="Allele.organism.taxonId" code="C" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele.genotypes.ontologyAnnotations.ontologyTerm" editable="false" type="MedicTerm"/>
    <constraint path="Allele" code="D" editable="true" op="LOOKUP" value="Kras*" extraValue=""/>
  </query>
</template>
<template name="MAllele_MPhenotype" title="Mouse alleles --&gt; Mammalian phenotypes (MP terms)" comment="">
  <query name="MAllele_MPhenotype" model="genomic" view="Allele.primaryIdentifier Allele.symbol Allele.name Allele.genotypes.ontologyAnnotations.ontologyTerm.identifier Allele.genotypes.ontologyAnnotations.ontologyTerm.name" longDescription="Returns phenotypes (MP terms) displayed by mice carrying specified alleles." sortOrder="Allele.genotypes.ontologyAnnotations.ontologyTerm.identifier asc" constraintLogic="A and C and D and B and E and F and G and H">
    <constraint path="Allele.genotypes.ontologyAnnotations.qualifier" code="H" editable="false" description="" op="IS NULL"/>
    <constraint path="Allele.genotypes.zygosity" code="G" editable="false" description="" op="!=" value="cx"/>
    <constraint path="Allele.isRecombinase" code="F" editable="false" description="" op="=" value="false"/>
    <constraint path="Allele.isWildType" code="E" editable="false" description="" op="=" value="false"/>
    <constraint path="Allele.genotypes.ontologyAnnotations.dataSets.name" code="C" editable="false" description="" op="=" value="MPTerm to Mouse Genotype Annotations from MGI"/>
    <constraint path="Allele.genotypes.organism.taxonId" code="A" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele.organism.taxonId" code="B" editable="false" description="" op="=" value="10090"/>
    <constraint path="Allele.genotypes.ontologyAnnotations.ontologyTerm" editable="false" type="MPTerm"/>
    <constraint path="Allele" code="D" editable="true" op="LOOKUP" value="Kras*" extraValue=""/>
  </query>
</template>
<template name="MFeature_GO" title="Mouse features --&gt; Functions (GO terms)" comment="">
  <query name="MFeature_GO" model="genomic" view="SequenceFeature.primaryIdentifier SequenceFeature.symbol SequenceFeature.name SequenceFeature.mgiType SequenceFeature.ontologyAnnotations.ontologyTerm.namespace SequenceFeature.ontologyAnnotations.qualifier SequenceFeature.ontologyAnnotations.ontologyTerm.identifier SequenceFeature.ontologyAnnotations.ontologyTerm.name SequenceFeature.ontologyAnnotations.evidence.code.code SequenceFeature.ontologyAnnotations.evidence.withText SequenceFeature.ontologyAnnotations.evidence.publications.mgiJnum SequenceFeature.ontologyAnnotations.evidence.publications.pubMedId SequenceFeature.ontologyAnnotations.evidence.publications.citation" longDescription="Returns GO terms associated with specified mouse features." sortOrder="SequenceFeature.primaryIdentifier asc" constraintLogic="A and C and D">
    <constraint path="SequenceFeature.organism.taxonId" code="D" editable="false" description="" op="=" value="10090"/>
    <constraint path="SequenceFeature.ontologyAnnotations.ontologyTerm" editable="false" type="GOTerm"/>
    <constraint path="SequenceFeature.ontologyAnnotations.dataSets.name" code="A" editable="false" description="" op="=" value="GOTerm to Mouse Marker Annotations from MGI"/>
    <constraint path="SequenceFeature" code="C" editable="true" op="LOOKUP" value="Bmp*" extraValue="M. musculus"/>
  </query>
